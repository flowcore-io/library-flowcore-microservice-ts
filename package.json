{
  "name": "@flowcore/microservice",
  "license": "MIT",
  "version": "4.0.1",
  "type": "commonjs",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "https://github.com/flowcore-io/library-flowcore-microservice-ts.git",
    "type": "git"
  },
  "keywords": [
    "flowcore",
    "microservice",
    "typescript",
    "nestjs",
    "datadog",
    "winston",
    "prometheus"
  ],
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "copyfiles README.md LICENSE.txt dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@jbiskur/nestjs-test-utilities": "5.0.2",
    "@nestjs/cli": "10.3.0",
    "@nestjs/common": "10.3.0",
    "@nestjs/core": "10.3.0",
    "@nestjs/microservices": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/terminus": "10.2.0",
    "@nestjs/testing": "10.3.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "commitizen": "4.3.0",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3",
    "zod": "3.22.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "3.0.0",
    "@jbiskur/nestjs-async-module": "5.0.2",
    "@jbiskur/nestjs-options-module-factory": "5.0.2",
    "@types/dotenv": "8.2.0",
    "@types/lodash": "4.14.195",
    "@willsoto/nestjs-prometheus": "5.2.0",
    "dotenv": "16.3.1",
    "lodash": "4.17.21",
    "nest-winston": "1.9.2",
    "prom-client": "14.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "winston": "3.9.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.1 || ^10.0.0",
    "@nestjs/core": "^9.2.1 || ^10.0.0",
    "@nestjs/terminus": "^9.1.4 || ^10.0.0",
    "zod": "^3.20.2"
  }
}
